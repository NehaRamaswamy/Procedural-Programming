   ],
   "source": [
    "num = 234\n",
    "rev = 0\n",
    "pow = len(str(num))-1\n",
    "while num != 0:\n",
    "    rem = num % 10\n",
    "    power = 10 ** pow\n",
    "    pow -= 1\n",
    "    rev += rem * power\n",
    "    num //= 10\n",
    "\n",
    "print('Reverse the number',rev)"
   ]
